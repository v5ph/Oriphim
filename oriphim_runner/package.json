    "clean": "rimraf src-tauri/target dist",
    "lint": "eslint src --ext .js,.html,.css",
    "format": "prettier --write src/**/*.{js,html,css,json}"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.6.3",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "options-trading",
    "ai",
    "automation",
    "desktop",
    "tauri",
    "ibkr",
    "finance"
  ],
  "author": "Oriphim Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oriphim/oriphim-runner"
  },
  "homepage": "https://oriphim.com",
  "tauri": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "./src",
    "distDir": "./src"
  }
